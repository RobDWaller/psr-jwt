{
    "name": "rbdwllr/psr-jwt",
    "description": "A PSR 7 compliant JSON Web Token Middleware Library.",
    "keywords": ["jwt", "json", "tokens", "authentication", "json web tokens", "php", "psr-7"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "RC",
    "authors": [
        {
            "name": "Rob Waller",
            "email": "rdwaller1984@googlemail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "rbdwllr/reallysimplejwt": "^2.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "nyholm/psr7": "^1.1"
    },
    "require-dev": {
    	"phpunit/phpunit": "^7.0",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-mockery": "^0.10",
        "phpmd/phpmd": "2.6.*",
        "squizlabs/php_codesniffer": "^3.0",
        "mockery/mockery": "^1.2",
        "infection/infection": "^0.12.2"
    },
    "autoload": {
        "psr-4": {
            "PsrJwt\\": "src",
            "Tests\\": "tests"
        }
    }
